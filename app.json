{
  "expo": {
    "name": "Le Contenent",
    "slug": "continent-news",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/continent_logo.jpg",
    "scheme": "Continent",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "runtimeVersion": {
      "policy": "appVersion"
    },

    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },

    "splash": {
      "image": "./assets/images/continent_logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "android": {
      "package": "com.wcucw.continentnews",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/continent_logo.jpg",
        "backgroundColor": "#E6F4FE"
      },
      "icon": "./assets/images/continent_logo.jpg",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["INTERNET"]
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/continent_logo.jpg"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/continent_logo.jpg",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "owner": "joa237",

    "extra": {
      "router": {},
      "eas": {
        "projectId": "e0168d1a-59be-400c-a3ad-4c0950a4a9f9"
      }
    }
  }
}
