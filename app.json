{
  "expo": {
    "name": "Continent",
    "slug": "continent-news",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/continent_logo.jpg",
    "scheme": "Continent",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/continent_logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wcucw.continentnews",
      "buildNumber": "1",
      "icon": "./assets/images/continent_logo.jpg",
      "infoPlist": {
        "NSCameraUsageDescription": "Cette application a besoin de la caméra.",
        "NSPhotoLibraryUsageDescription": "Accès aux images pour la publication.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.wcucw.continentnews",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/continent_logo.jpg",
        "backgroundColor": "#E6F4FE"
      },
      "icon": "./assets/images/continent_logo.jpg",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/continent_logo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/continent_logo.jpg",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser",
      "expo-audio",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "owner": "joa237",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e0168d1a-59be-400c-a3ad-4c0950a4a9f9"
      }
    }
  }
}
